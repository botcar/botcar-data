    <!-- Botanica Caroliniana Collections -->
    

    
    <citeCollection 
        urn="urn:cite:botcar:sloane" 
        canonicalId="specimenURN"
        label="name">
        <namespaceMapping abbr="botcar" fullValue="http://classics.furman.edu/datans"/>
        <dc:title>Botanica Caroliniana: Catesby Specimens</dc:title>
        <dc:description>BC: Specimens in Mark Catesbys H.S. volumes from the Sloane Herbarium</dc:description>
        <dc:rights>All data in this collection are available
            under the terms of the Creative Commons Attribution-Non-Commercial 3.0 Unported License,
            http://creativecommons.org/licenses/by-nc/3.0/deed.en_US</dc:rights>
        <source
            type="file"
            value="catesbySpecimens.csv"/>
        
        <citeProperty label="Specimen ID" name="specimenURN" type="citeurn"/>
        <citeProperty label="Name" name="name" type="string"/>
        <citeProperty label="Scientific Name" name="scientificName" type="string"/>
        <citeProperty label="Species URN" name="speciesURN" type="citeurn"/>
        <citeProperty label="Common Name" name="commonName" type="string"/>
        <citeProperty label="BC Genus URN" name="genusURN" type="citeurn"/>
        <citeProperty label="BC Family URN" name="familyURN" type="citeurn"/>
        <citeProperty label="BC Folio URN" name="folioURN" type="citeurn"/>
        <citeProperty label="Folio Image" name="folioImage" type="citeimg"/>
        <citeProperty label="Folio Image ROI" name="folioImageROI" type="citeimg"/>
        <citeProperty label="ITIS TSN Serial No." name="tsnID" type="string"/>
    </citeCollection>
   
    
    <citeCollection 
        urn="urn:cite:botcar:species"
