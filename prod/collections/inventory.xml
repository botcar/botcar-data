            value="species.csv"/>
        <citeProperty label="Species ID" name="speciesURN" type="citeurn"/>
        <citeProperty label="Family ID" name="familyURN" type="citeurn"/>
        <citeProperty label="Genus ID" name="genusURN" type="citeurn"/>
        
        <citeProperty label="Scientific Name (markdown)" name="scientificName" type="string"/>
        
        <citeProperty label="Label" name="label" type="string"/>
        <citeProperty label="ITIS TSN Serial No." name="tsn" type="string"/>
